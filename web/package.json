    "main": "index.js",
    "scripts": {
        "getmodules-lesscode": "cd .. && git subtree add --prefix=web/src/pages/lesscode git@github.com:olaola-chat/bms-lesscode-web.git master --squash",
        "updatemodules-lesscode": "cd .. && git subtree pull --prefix=web/src/pages/lesscode git@github.com:olaola-chat/bms-lesscode-web.git master --squash",
        "pushmodules-lesscode": "cd .. && git subtree push --prefix=web/src/pages/lesscode git@github.com:olaola-chat/bms-lesscode-web.git master",
        "clean-lesscode": "rm -rf ./src/pages/lesscode",
        "start": "cross-env VEEKA_ENV=local node scripts/start.js",
        "start:test": "cross-env VEEKA_ENV=test node scripts/start.js",
        "build:test": "sh scripts/build.sh test",
        "build:prod": "sh scripts/build.sh prod",
        "build:dll": "rimraf scripts/dll && webpack --config scripts/config/webpack.dll.config.js",
        "build:analyz": "cross-env ANALYZ=true npm run build",
        "build:time": "cross-env ANALYZ_TIME=true npm run build",
        "build-test": "cross-env VEEKA_ENV=test node scripts/build.js",
        "build-prod": "cross-env VEEKA_ENV=prod node scripts/build.js"
    },
